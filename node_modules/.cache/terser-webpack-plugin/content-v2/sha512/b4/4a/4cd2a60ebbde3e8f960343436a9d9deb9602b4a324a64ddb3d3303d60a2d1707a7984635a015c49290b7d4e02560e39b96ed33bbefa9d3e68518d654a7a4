/asset/ryu_idle_right.png\\\";\\n    } else {\\n      this.image.src = \\\"./asset/ryu_idle_left.png\\\";\\n    }\\n    this.fpsControl(10);\\n    this.ctx.drawImage(this.image,this.frame * this.imageW, 0 , this.imageW, 90, this.x, this.y, this.w, this.h);\\n  }\\n\\n  comboText() {\\n    this.ctx.font=\\\"20px Acme\\\";\\n    this.ctx.fillStyle = \\\"black\\\"\\n    this.ctx.textAlign = \\\"center\\\"\\n    this.ctx.fillText(`Hit! Combo`,this.x,this.y - 50);\\n    this.firsthit = true;\\n  }\\n  dead(x) {\\n    this.ctx.save()\\n    this.ctx.globalAlpha = this.transparent;\\n    this.transparent -= 0.1;\\n    this.comboText();\\n    if (x < this.x) {\\n      this.image.src = \\\"./asset/ryu_stuned_right.png\\\";\\n    } else {\\n      this.image.src = \\\"./asset/ryu_stuned_left.png\\\";\\n    }\\n    if (this.frame >= this.maxFrame - 1) this.frame = 0;\\n    this.ctx.drawImage(this.image,this.frame * this.imageW, 0 , this.imageW, 90, this.x, this.y, this.w, this.h);\\n    this.ctx.restore();\\n  }\\n  draw(x) {\\n    if (this.alive) {\\n      this.setup(x)\\n    } else {\\n      this.dead(x)\\n    }\\n  }\\n\\n\\n}\\n\",\"import Player from './player.js';\\nimport Background from './background.js';\\nimport Enemy from './enemy.js';\\nimport Input from './input.js'\\nimport Sound from './sound.js'\\n\\nclass Game {\\n  constructor(ctx) {\\n    this.ctx = ctx;\\n    this.player = new Player(this.ctx);\\n    this.background = new Background(this.ctx);\\n    this.enemy = new Enemy(this.background.x + Math.floor(Math.random() * 5) * 150, this.ctx);\\n    this.play = this.play.bind(this);\\n    this.input = new Input();\\n    this.input.gameControl();\\n    this.combo = 1;\\n    this.maxCombo = 0\\n    this.firsthit = false;\\n    this.time = 60;\\n    this.fps = 0;\\n    this.comboTime = 200;\\n    this.kill = 0;\\n    this.comboSpeed = 0.7;\\n    this.image = new Image();\\n    this.sound = new Sound();\\n    this.image.src = 'https://i.pinimg.com/originals/f6/c4/51/f